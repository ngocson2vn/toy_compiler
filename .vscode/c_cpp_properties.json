{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/local/cuda-12.4/include",
                "${workspaceFolder}/llvm-project/mlir/include",
                "${workspaceFolder}/llvm-project/llvm/include",
                "${workspaceFolder}/llvm-project/lld",
                "${workspaceFolder}/llvm-project/lld/include",
                "${workspaceFolder}/llvm-project/build/include",
                "${workspaceFolder}/llvm-project/build/tools/mlir/include",
                "${workspaceFolder}",
                "${workspaceFolder}/build"
            ],
            "defines": [
                "GOOGLE_CUDA",
                "GET_OP_CLASSES",
                "GEN_PASS_REGISTRATION"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "browse": {
                "path": [
                    "/usr/local/cuda-12.4/include",
                    "${workspaceFolder}/llvm-project/mlir/include/**",
                    "${workspaceFolder}/llvm-project/llvm/include",
                    "${workspaceFolder}/llvm-project/lld",
                    "${workspaceFolder}/llvm-project/lld/include",
                    "${workspaceFolder}/llvm-project/build/include",
                    "${workspaceFolder}/llvm-project/build/tools/mlir/include",
                    "${workspaceFolder}",
                    "${workspaceFolder}/build"
                ],
                "databaseFilename": "${workspaceFolder}/.vscode/vc.db",
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}

